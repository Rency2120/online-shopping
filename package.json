{
  "name": "ninja-products",
  "version": "1.0.0",
  "description": "TypeScript + MySql  + Express API Server",
  "author": "rencypatel21@gmail.com",
  "license": "ISC",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only foreplay_service_prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "seed:category": "ts-node-dev -r tsconfig-paths/register src/Seeder/categorySeeder.ts",
    "seed:subCategory": "ts-node-dev -r tsconfig-paths/register src/Seeder/subCategory.ts",
    "seed:product": "ts-node-dev -r tsconfig-paths/register src/Seeder/productSeeder.ts",
    "seed:productVariant": "ts-node-dev -r tsconfig-paths/register src/Seeder/productVariantSeeder.ts "
  },
  "dependencies": {
