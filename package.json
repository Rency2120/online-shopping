    "express-validator": "^7.2.0",
    "google-auth-library": "^9.8.0",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
